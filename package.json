{
  "name": "g.js",
  "version": "0.0.2",
  "description": "Graphic Objects Library",
  "main": "src/g.js",
  "scripts": {
    "test": "mocha --recursive && jshint .",
    "watchify": "mkdir -p build && watchify src/g.js --standalone g --debug -o build/g.js -v",
    "browserify": "browserify src/g.js --standalone g > dist/grob.js",
    "uglify": "browserify src/g.js --standalone g -g uglifyify > dist/g.min.js",
    "dist": "npm run test && npm run browserify && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodebox/grob.git"
  },
  "keywords": [
    "vector",
    "graphics",
    "canvas",
    "image",
    "pixels",
    "2d"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodebox/grob/issues"
  },
  "homepage": "https://github.com/nodebox/grob",
  "dependencies": {
    "async": "~0.2.10",
    "lodash": "^3.7.0",
    "xmldom": "^0.1.19",
    "js-clipper": "^1.0.1",
    "stackblur": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "jshint": "^2.7.0",
    "mocha": "^2.2.4",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.2"
  }
}
